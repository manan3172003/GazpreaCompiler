/*
This test validates type error: or operator on floats.
Tests: const r_or = r1 or r2; should fail
*/
typealias integer int;
typealias boolean bool;
typealias real float;
typealias character char;

procedure main() returns integer {
    float r1 = 10.0;
    const r2 = 2.5;

    const r_or = r1 or r2;   // should TypeError

    return 0;
}

