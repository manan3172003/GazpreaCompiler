/*
Procedure call should throw aliasing error
*/

procedure swap(var integer a, var integer b) { var integer t = a; a = b; b = t; }

procedure main() returns integer {
  var integer q = 4;
  var tuple(integer, integer) t = (q, 5);
  call swap(t.1, t.1);   // both arguments alias same lvalue
  return 0;
}

