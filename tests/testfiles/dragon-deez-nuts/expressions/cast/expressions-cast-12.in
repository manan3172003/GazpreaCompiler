procedure main() returns integer {
    real[3] v = [i in 1..3 | i + 0.3 * i];

    integer[3] u = as<integer[*]>(v);
    integer[5] x = as<integer[5]>(v);
    real[2] y = as<real[2]>(v);

    v -> std_output;
    u -> std_output;
    x -> std_output;
    y -> std_output;

    return 0;
}
