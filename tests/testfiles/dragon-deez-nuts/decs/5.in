const tuple(integer, integer) z1 = (1, 5);
const tuple(real, real) z2 = (1, 5); // implicit cast on global dec

procedure main() returns integer {
    tuple(integer, integer) a = (3, 6);
    const b = (98, 'c'); // inferred as tuple
    tuple(real, real) c = (98, 1); // implicit cast int to real
    a.1 -> std_output;
    a.2 -> std_output;
    b.1 -> std_output;
    b.2 -> std_output;
    c.1 -> std_output;
    c.2 -> std_output;
    z1.1 -> std_output;
    z1.2 -> std_output;
    z2.1 -> std_output;
    z2.2 -> std_output;
    return 0;
}
