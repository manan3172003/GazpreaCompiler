/*
Test tuple equality operations
*/
procedure main() returns integer {
    // Tuple equality
    tuple(integer, integer) a = (1, 4);
    tuple(integer, integer) b = (1, 6);
    tuple(integer, integer) g = (6, 4);
    tuple(integer, real) c = (1, 4);
    tuple(integer, real, character) d = (1, 4, 'c');
    tuple(boolean, real, character) e = (false, 4, 'c');
    tuple(boolean, real, boolean) h = (false, 4, true);

    (a == b) -> std_output; // F
    (a == c) -> std_output; // T
    (a != b) -> std_output; // T
    (a != c) -> std_output; // F
    (a != d) -> std_output; // T
    (c != c) -> std_output; // F
    (d == d) -> std_output; // T
    (a == a) -> std_output; // T

    (a != g) -> std_output; // T
    (a == g) -> std_output; // F
    (d == e) -> std_output; // F
    (d != e) -> std_output; // T

    (h != e) -> std_output; // T
    (h == e) -> std_output; // F

    return 0;
}
//CHECK:FTTFTFTTTFFTTF
