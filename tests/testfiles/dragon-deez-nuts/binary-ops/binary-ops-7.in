/*
Test mixed integer-real operations (implicit casting)
*/
procedure main() returns integer {
    // Mixed integer-real arithmetic
    integer i = 10;
    real r = 3.5;

    (i + r) -> std_output;   // 13.5 (int -> real)
    (r + i) -> std_output;   // 13.5 (int -> real)
    (i - r) -> std_output;   // 6.5
    (i * r) -> std_output;   // 35.0
    (i / r) -> std_output;   // 2.85714

    return 0;
}
//CHECK:13.513.56.5352.85714
