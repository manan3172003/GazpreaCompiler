/*
Test real arithmetic operations
*/
procedure main() returns integer {
    // Real arithmetic
    real a = 10.5;
    real b = 2.0;

    (a + b) -> std_output;   // 12.5
    (a - b) -> std_output;   // 8.5
    (a * b) -> std_output;   // 21.0
    (a / b) -> std_output;   // 5.25

    return 0;
}
//CHECK:12.5000008.50000021.0000005.250000